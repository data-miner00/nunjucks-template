{
  "name": "nunjucks-template",
  "version": "1.0.0",
  "description": "A Gulp template to work with Nunjucks",
  "scripts": {
    "build": "gulp",
    "tasks": "gulp --tasks",
    "dev": "gulp live",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/tailwind.css",
    "dev:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/tailwind.css --watch"
  },
  "keywords": [
    "gulp",
    "nunjucks",
    "css"
  ],
  "author": "Shaun Chong <mumk0313@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-data": "^1.3.1",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-uglify": "^3.0.2",
    "prettier": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "through2": "^4.0.2"
  }
}
